pipeline {
    agent any
    stages {
        stage('One') {
                steps {
                    echo 'Hi, this is Aman from cbnits'
                }
        }
            stage('Two'){
                steps {
                        input('do you want to proceed?')
                }
            }
            stage('Three') {
                when {
                        not {
                                branch "master"
                        }
                }
                steps {
                    echo "Hello"
                }
            }
            
    }
}
